<plugin
   id="org.eclipse.build.tools"
   name="org.eclipse.build.tools"
   version="1.0.0">

   <runtime>
   </runtime>

   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry
            library="buildTools.jar">
      </extraClasspathEntry>
      <extraClasspathEntry
            library="feedTools.jar">
      </extraClasspathEntry>
   </extension>
	<!-- Tasks -->
